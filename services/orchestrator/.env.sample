# Service Orchestrator Configuration
# All configuration is loaded from config.yml
# Environment variables can be used to override specific settings

# Optional: Override transporter (default from config.yml)
# ORCHESTRATOR_TRANSPORTER=null
# ORCHESTRATOR_TRANSPORTER=nats://localhost:4222

# Optional: Override log level (default from config.yml)
# LOG_LEVEL=debug

# Optional: Override metrics port (default: 3030)
# METRICS_PORT=3030

# Optional: Override environment
# NODE_ENV=development

# Optional: Override Core Apps GraphQL URL
# PLATFORM_INVENTORY_GRAPHQL_URL=http://localhost:4000/graphql
# PLATFORM_GRPC_HOST=platform:50051
