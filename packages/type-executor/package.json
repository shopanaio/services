{
  "name": "@shopana/type-executor",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "description": "Recursive data resolution library similar to GraphQL, using classes as types with resolvers",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./graphql": {
      "types": "./dist/utils/graphqlArgsParser.d.ts",
      "import": "./dist/utils/graphqlArgsParser.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc -p tsconfig.json --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "type-executor",
    "graphql",
    "resolver",
    "typescript",
    "dataloader"
  ],
  "author": "Shopana.io",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "graphql": "^16.0.0",
    "graphql-parse-resolve-info": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    },
    "graphql-parse-resolve-info": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "graphql": "^16.9.0",
    "graphql-parse-resolve-info": "^4.14.0",
    "typescript": "^5.4.5",
    "vitest": "^2.1.8"
  }
}
