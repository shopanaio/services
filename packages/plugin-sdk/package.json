{
  "name": "@shopana/plugin-sdk",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.17"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./shipping": {
      "types": "./dist/shipping.d.ts",
      "default": "./dist/shipping.js"
    },
    "./payment": {
      "types": "./dist/payment.d.ts",
      "default": "./dist/payment.js"
    },
    "./pricing": {
      "types": "./dist/pricing.d.ts",
      "default": "./dist/pricing.js"
    },
    "./inventory": {
      "types": "./dist/inventory.d.ts",
      "default": "./dist/inventory.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch --preserveWatchOutput",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@shopana/shared-money": "workspace:*",
    "bottleneck": "^2.19.5",
    "cockatiel": "^3.2.1",
    "got": "^14.4.8",
    "semver": "^7.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/semver": "^7.7.1",
    "rimraf": "^5.0.0",
    "typescript": "^5.5.4"
  }
}
