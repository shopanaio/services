{
  "name": "@shopana/checkout-sdk",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@shopana/shared-money": "*",
    "@shopana/shipping-plugin-sdk": "*"
  },
  "devDependencies": {
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.4"
  }
}
